pipelineCalculadora
pipeline
{
	agent any
	
	stages
	{
		stage('Checkout')
		{
			steps
			{
				checkout scm
			}
		}
		stage('Restore')
		{
			steps
			{
				sh 'dotnet restore projectClaculadora.sln'	
			}
		}
		stage('Build')
		{
			steps
			{
				sh 'dotnet build projectClaculadora.sln --confguration Release --no-build'	
			}
		}
		stage('Test')
		{
			steps
			{
				sh 'dotnet test calculadoraApp.Tests/calculadoraApp.Tests.csproj --confguration Release --no-build'	
			}
		}
		stage('Publsh')
		{
			steps
			{
				sh 'dotnet pulish calculadoraApp/calculadoraApp.csproj -c Release -o publish'	
			}
		}


	}
	post
	{
		success
		{
			archiveArtifacts artifacts: 'publish/**', fingerprint:true
		}
	

}

